          <fr:frontmatter>
            <fr:authors>
              <fr:author>Ana Romero and Francis Sergeraert</fr:author>
            </fr:authors>
            <fr:date>
              <fr:year>2010</fr:year>
              <fr:month>5</fr:month>
              <fr:day>31</fr:day>
            </fr:date>
